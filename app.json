{
  "expo": {
    "name": "Atomix",
    "slug": "Atomix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": ""
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://atomix.dev/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
